{
    "name": "ricardoaraujo/boas-praticas-bots",
    "description": "Exemplos de boas praticas p/ serem aplicadas aos projetos de bots e crawler em geral",
    "type": "project",
    "authors": [
        {
            "name": "Ricardo Araujo",
            "email": "ricardo.araujo@forseti.com.br"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "symfony/dom-crawler": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "BoasPraticas\\Bots\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoasPraticas\\Bots\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
